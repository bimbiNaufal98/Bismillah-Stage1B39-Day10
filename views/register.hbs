<body>
    <nav class="navbar navbar-expand-lg navbar-light bg-light">
        <div class="container">
            <img src="https://dumbways.id/assets/images/brandred.png" style="width: 60px; margin-right: 35px;"/>
            <button class="navbar-toggler" type="button" data-bs-toggle="collapse"
                data-bs-target="#navbarSupportedContent" aria-controls="navbarSupportedContent" aria-expanded="false"
                aria-label="Toggle navigation">
                <span class="navbar-toggler-icon"></span>
            </button>
            <div class="collapse navbar-collapse" id="navbarSupportedContent">
                <ul class="navbar-nav me-auto mb-2 mb-lg-0">
                    <li class="nav-item">
                        <a class="nav-link text-home" aria-current="page" href="/">Home</a>
                        <!-- nav link active utk membuat link menjadi aktif terus, saat tidak di klik pun menyala -->
                    </li>
                    {{#if isLogin}}
                        <li class="nav-item">
                            <a class="nav-link text-orange" href="/myproject">Add Project</a>
                        </li>
                    {{/if}}
                </ul>
                {{#if isLogin}}
                    <a href="/#" class="btn btn-success bg-orange2 px-1" style="font-family: 'Segoe UI';"><span>Hello, {{user.name}}</span></a>
                    <a href="/logout" class="text-danger mx-3 bg-orange3">Logout</a>
                {{else}}
                    <a href="/register" class="bg-orange1 px-1" style="font-family: 'Segoe UI';"><span>Register</span></a>
                    <a href="/login" class="bg-orange1 px-1" style="font-family: 'Segoe UI';"><span>Login</span></a>
                {{/if}}
                <a href="/contact" class="bg-orange text-light px-1"><span>Contact Me</span></a>
                <!-- p itu adalah padding, px itu padding kanan kiri, py itu padding atas bawah, dan maksimal semua di 5 -->
            </div>
        </div>
    </nav>
    <div class="form-submit1">
        <form action="/register" method="post">
            <div class="judul">FORM REGISTER</div>
            <div class="project-name">
                <label for="inputName">Name</label>
                <input class="form-control" id="inputName" name="inputName" type="text">
            </div>
            <div class="project-name">
                <label for="inputEmail">Email</label>
                <input class="form-control" id="inputEmail" name="inputEmail" type="email">
                <div id="emailHelp" class="form-text">We'll never share your email with anyone else.</div>
            </div>
            <div class="project-name">
                <label for="inputPassword">Password</label>
                <input class="form-control" id="inputPassword" name="inputPassword" type="password">
                <div id="emailHelp" class="form-text">Please fill in at least 8 characters</div>
            </div>
            <button type="submit" class="bg-color">Register</button>
        </form>
    </div>
